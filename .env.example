DATABASE_URL=postgresql://postgres:your_password_here@your_host:5432/flarenet?schema=public

# Redis configuration
# For Redis Cloud, use the full URL format:
REDIS_URL=redis://:your_password_here@your-redis-host.redns.redis-cloud.com:your-port
# Set to 'true' if Redis connection requires TLS (often needed for cloud services)
REDIS_USE_TLS=true
# Legacy Redis configuration (used as fallback if REDIS_URL is not provided)
REDIS_HOST=your-redis-host
REDIS_PORT=your-port
REDIS_PASSWORD=your_password_here
# Set to 'false' to disable Redis completely
REDIS_ENABLED=true

CLICKHOUSE_HOST=http://localhost:8123
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=your_password_here
KAFKA_BROKER=localhost:9092
